#scrape https://www.bhphotovideo.com/c/search?Ntt=35mm%20film&N=0&InitialSearch=yes&sts=ma&Top+Nav-Search= for film prices
import requests
from bs4 import BeautifulSoup

url = "https://www.bhphotovideo.com/c/search?Ntt=35mm%20film&N=0&InitialSearch=yes&sts=ma&Top+Nav-Search="
response = requests.get(url)
soup = BeautifulSoup(response.text, 'html.parser')

# Scraping logic to extract film prices would go here
for product in soup.find_all("div", class_="itemInfo"):
    product_name = product.find("a", class_="productTitle").text.strip().lower()
    if "hp5" in product_name:
        product_price = product.find("div", class_="price_1uvXb").text
        print(f"Price for product {product_name}: {product_price}")
